<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="jQuery" Version="3.7.1" />
    </ItemGroup>

    <ItemGroup>
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\about.txt" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\android-chrome-192x192.png" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\android-chrome-512x512.png" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\apple-touch-icon.png" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\favicon-16x16.png" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\favicon-32x32.png" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\favicon.io" />
      <_ContentIncludedByDefault Remove="wwwroot\img\favicon_io\site.webmanifest" />
    </ItemGroup>

</Project>
